<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>TrafficServlet</servlet-name>
		<servlet-class>com.elolozone.trafficServlet.TrafficServletServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>DeleteTable</servlet-name>
		<servlet-class>com.elolozone.trafficServlet.DeleteTable</servlet-class>
	</servlet>
	<servlet-mapping> 
		<servlet-name>TrafficServlet</servlet-name>
		<url-pattern>/trafficservlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>DeleteTable</servlet-name>
		<url-pattern>/deletetable</url-pattern>
	</servlet-mapping>
